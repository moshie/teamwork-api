{
  "name": "teamwork-api",
  "version": "0.0.0-development",
  "description": "A wrapper around teamwork api making it easier to perform requests.",
  "keywords": [
    "api",
    "teamwork",
    "projects"
  ],
  "homepage": "https://github.com/moshie/teamwork-api#readme",
  "bugs": {
    "url": "https://github.com/moshie/teamwork-api/issues"
  },
  "license": "MIT",
  "author": "David Hewitt <hello@wp-unite.io> (https://www.way2adv.com/)",
  "main": "src/index.js",
  "directories": {
    "doc": "./docs",
    "lib": "./src",
    "test": "./test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moshie/teamwork-api.git"
  },
  "scripts": {
    "commit": "npm run docs && git-cz",
    "lint": "eslint src",
    "docs": "node ./docs.js",
    "test": "istanbul cover _mocha -- -R spec",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "axios": "^0.21.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "comment-parser": "^0.6.2",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.15.0",
    "ghooks": "^2.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "nock": "^9.1.6",
    "recursive-readdir": "^2.2.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^15.14.0",
    "travis-deploy-once": "^5.0.11"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "prettier": {
    "trailingComma": "none"
  }
}
